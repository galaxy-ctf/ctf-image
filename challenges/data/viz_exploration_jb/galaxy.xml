<?xml version="1.0"?>
<root>
    <metadata>
        <gencode>1</gencode>
        <genomes>
                  <genome>out.fa</genome>
        </genomes>
        <general>
            <defaultLocation></defaultLocation>
            <trackPadding>20</trackPadding>

            <shareLink>true</shareLink>
