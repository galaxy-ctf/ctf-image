                        <method>score</method>
                        <algo>linear</algo>
                        <scales>
                            <type>automatic</type>

                        </scales>
                        <scheme>
                            <type>opacity</type>
                            <color>__auto__</color>
                        </scheme>
                </scaling>
                <menus>
                </menus>

                <gff>
                    <trackType>JBrowse/View/Track/CanvasFeatures</trackType>
                    <index>false</index>
                </gff>
            </options>
        </track>
    </tracks>
